<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Programs</title>
    <link rel="stylesheet" type="text/css" href="styles.css">
</head>
<body>
    <h1><%= response.music_title %></h1>

    <table id="programTable">
        <thead>
            <tr>
                <th>Skater</th>
                <th>Country</th>
                <th>Discipline</th>
                <th>Program Type</th>
                <th>Season</th>
                <th>Choreographer</th>
            </tr>
        </thead>
        <tbody id="programTableBody">
            <% response.programs.forEach(program => { %>
                <tr>
                    <td><%= program.skater_name %></td>
                    <td><%= program.skater_country %></td>
                    <td><%= program.discipline_type %></td>
                    <td><%= program.program_type %></td>
                    <td><%= program.season %></td>
                    <td><%= program.choreographer_name %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const response = <%- JSON.stringify(response) %>;
        });
    </script>
</body>
</html>
